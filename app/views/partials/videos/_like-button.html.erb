<% pre_like = video.likes.find { |like| like.user_id == current_user.id} %>
<% if pre_like %>
    <%= button_to 'Unlike', video_like_path(video, pre_like), method: :delete, id: "videounlikebutton", remote: true %>
<% else %>
    <%= button_to 'Like', video_likes_path(video), method: :post, id: "videolikebutton", remote: true %>
<% end %>